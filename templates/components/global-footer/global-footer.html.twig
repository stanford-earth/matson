{#
/**
* @file
* Default theme implementation for the basic structure of the global stanford footer
*/
#}

<footer id="footer__container">
  <section class="contact-footer" style="background-image:url('{{ base_path ~ directory }}/img/optimized/footer-bg.png')";>
    <div class="contact-footer__brand">
      {{ drupal_block('matson_branding')|render|preg_replace("block-matson-branding", "footer__block-matson-branding")|raw }}
    </div>
    <div class="contact-footer__social">
      <h2>{{ 'Join us on Social Media'|trans }}</h2>
      <ul class="social-links">
        <li><a href="https://twitter.com/stanfordearth"><span class="visually-hidden">Twitter</span>
          <svg class="i-svg i-svg--twitter is-white" role="img"><title>{{ 'Twitter'|t }}</title><use xlink:href="#i-twitter" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg>
        </a></li>
        <li><a href="https://www.facebook.com/stanfordearth"><span class="visually-hidden">Facebook</span>
          <svg class="i-svg i-svg--fb is-white" role="img"><title>{{ 'Facebook'|t }}</title><use xlink:href="#i-fb" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a></li>
          <li><a href="https://www.instagram.com/stanfordearth/"><span class="visually-hidden">Instagram</span>
            <svg class="i-svg i-svg--instagram is-white" role="img"><title>{{ 'Instagram'|t }}</title><use xlink:href="#i-instagram" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg>
          </a></li>
        </ul>
    </div>
    <div class="contact-footer__links">
      <h2>{{ 'Links'|trans }}</h2>
      {{ drupal_menu('footer') }}
    </div>
    <div class="contact-footer__subscribe">
      <h2>{{ 'Subscribe to Earth Matters'|trans }}</h2>
      <p>{{ 'Our Monthly Research News Alert'|trans }}</p>
      <div id="mc_embed_signup">
        <form action="//stanford.us7.list-manage.com/subscribe/post?u=167e9da7acec83c4cc802b3e7&amp;id=69d6fa9e3a" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate subscribe-form" target="_blank" novalidate>
          <div id="mc_embed_signup_scroll">
            <input type="email" value="" name="EMAIL" class="email email-input" id="mce-EMAIL" class="email-input" placeholder="name@stanford.edu" required>
            <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
            <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_167e9da7acec83c4cc802b3e7_69d6fa9e3a" tabindex="-1" value=""></div>
            <div class="clear"><input type="submit" value="Subscribe" name="{{ 'Join'|trans }}" id="mc-embedded-subscribe" class="button button__hollow"></div>
          </div>
        </form>
      </div>
    </div>
  </section>

  <section id="footer__global-footer">
    <div class="global-footer__inner">
      <div id="global-footer__brand">
        <object alt="Stanford University" data="{{ base_path ~ directory }}/assets/svg/su_sig_stack_linked.svg" height="49" tabindex="-1" width="105">Stanford University Logo</object>
      </div>

      <div id="global-footer__nav" role="navigation">
        <nav id="nav__global-links">
          <a href="https://www.stanford.edu">Stanford Home</a>
          <a href="https://visit.stanford.edu/plan/maps.html">Maps &amp; Directions</a>
          <a href="https://stanford.edu/search/">Search Stanford</a>
          <a href="https://www.stanford.edu/site/terms.html">Terms of Use</a>
          <a href="https://emergency.stanford.edu/">Emergency Info</a>
        </nav>
      </div>

      <div id="global-footer__info">
        <span class="vcard">Â©
          <span class="fn org">Stanford University</span>.
          <span class="adr">
            <span class="locality">Stanford</span>,
            <span class="region">California</span>
            <span class="postal-code">94305</span>.
          </span>
          <span class="copyright-links">
            <a href="http://www.stanford.edu/site/privacy.html">Privacy Policy</a>
            <a href="https://uit.stanford.edu/security/copyright-infringement">Copyright Complaints</a>
            <a href="https://adminguide.stanford.edu/chapter-1/subchapter-5/policy-1-5-4">Trademark Notice</a>
          </span>
        </span>
      </div>
    </div>
  </section>
</footer>

